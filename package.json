{
  "name": "flat-map",
  "version": "0.0.0",
  "author": "Jonathan Sewell",
  "main": "server.js",
  "scripts": {
    "installdeps" : "npm install && bower install",
    "prestart" : "export NODE_ENV='production' && npm run installdeps && webpack",
    "start": "node .",
    "dev": "npm run installdeps && webpack-watch",
    "nodemon": "nodemon --ignore build/ --ignore public/ --verbose",
    "webpack-watch": "webpack --watch --colors --progress",
    "test": "mocha --compilers js:babel-core/register --recursive"
  },
  "dependencies": {
    "babel-core": "6.4.5",
    "babel-loader": "6.2.1",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "eslint": "1.10.3",
    "exports-loader": "0.6.2",
    "expose-loader": "0.7.1",
    "express": "4.13.4",
    "imports-loader": "0.6.5",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "webpack": "1.12.12",
    "bower": "1.7.7",
    "eslint-plugin-react": "3.16.1",
    "mocha": "2.4.5",
    "nodemon": "1.8.1",
    "sinon": "1.17.3"
  }
}
